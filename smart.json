{
  "global": {
    "text": {
      "description": "The Ten Commandments For Exceptional Developers",
      "title": "The 10 Commandments",
      "for": "Exceptional Developers",
      "command1": "do build on the great work of others, . . don't reinvent the wheel",
      "command2": "do architect a solution from the actual problem, . . don't rush into writing throw-away code",
      "command3" : "do use the best technology for the job, . . don't force your square peg into every round hole",
	    "command4" : "do follow software development best practices, . . don't build on assumptions",
	    "command5" : "do consider business requirements, . . don't simply focus on generating code",
	    "command6" : "do write modular code & share across open source communities, . . don't hoard value",
	    "command7" : "do keep up to date with new technologies, . . don't assume you know enough",
	    "command8" : "do become a unicorn with front and back-end knowledge, . . don't stay comfortable",
	    "command9" : "do learn to be proficient at command line, . . don't rely on tools or make excuses",
      "command10" : "do practise this list daily, make your team smarter, . . don't wait for opportunities",
      "footer" : "For your daily revelations, visit :",
      "url" : "http://websemantics.ca"
    },
    "color": {
      "white": "#ffffff",
      "black": "#000000",
      "fill1": "#f573b5",
      "fill2": "#c884c8",
      "fill3": "#9dcdef",
      "fill4": "#83d3bf",
      "fill5": "#c1e695",
      "fill6": "#d5e95b",
      "fill7": "#fbf888",
      "fill8": "#fbc669",
      "fill9": "#f89a86",
      "fill10": "#f2583a",
      "background": "#201e1d"
    },
  "files": {
      "dist": "dist",
      "dest": "assets",
      "source": "src",
      "concatenator": "-"
    },
    "default": {
      "none": "none",
      "width": 730,
      "height": 520,
      "zoom": 0,
      "transform": "translate(0)"
    },
    "bot": {
      "name": "iAyeBot",
      "email": "iayebot@websemantics.ca",
      "url": "http:\/\/github.com\/iayebot",
      "img": "https:\/\/avatars1.githubusercontent.com\/u\/18201639?v=3&amp;s=40"
    }
  },
  "template": [
    {
      "description": "{{ global.text.description}}",
      "source": "{{ global.files.source }}\/logo.template",
      "dest": "{{ global.files.dest }}",
      "data":["commandments"],
      "svg": {
        "dest": "svg"
      },
      "png": {
        "dest": "png",
        "scale": {
          "width": [1920]
        },
        "retina": true
      }
    }
  ],
  "html": {
    "dest": ".",
    "template": "{{ global.files.source }}\/index.template",
    "filename": "index.html",
    "title": "{{ global.text.title}}",
    "description": "{{ package.description }}",
    "github_url": "{{& package.repository }}",
    "link": {
      "text": "Visit Website \u00bb",
      "url": "{{& package.repository.url }}"
    },
    "generated_by": {
      "text": "Generated by,",
      "name": "{{ global.bot.name }}",
      "url": "{{& global.bot.url }}",
      "img": "{{& global.bot.img }}"
    }
  },
  "data": {
    "default": {
      "name": "",
      "value": {
        "zoom": "{{ global.default.zoom }}",
        "width": "{{ global.default.width }}",
        "height": "{{ global.default.height }}",
        "transform": "{{ global.default.transform }}",
        "background_fill": "{{ global.color.background }}",
        "fill1": "{{ global.color.fill1 }}",
        "fill2": "{{ global.color.fill2 }}",
        "fill3": "{{ global.color.fill3 }}",
        "fill4": "{{ global.color.fill4 }}",
        "fill5": "{{ global.color.fill5 }}",
        "fill6": "{{ global.color.fill6 }}",
        "fill7": "{{ global.color.fill7 }}",
        "fill8": "{{ global.color.fill8 }}",
        "fill9": "{{ global.color.fill9 }}",
        "fill10": "{{ global.color.fill10 }}",
        "title": "{{ global.text.title }}",
        "for": "{{ global.text.for }}",
        "command1": "{{{ global.text.command1 }}}",
        "command2": "{{{ global.text.command2 }}}",
        "command3": "{{{ global.text.command3 }}}",
        "command4": "{{{ global.text.command4 }}}",
        "command5": "{{{ global.text.command5 }}}",
        "command6": "{{{ global.text.command6 }}}",
        "command7": "{{{ global.text.command7 }}}",
        "command8": "{{{ global.text.command8 }}}",
        "command9": "{{{ global.text.command9 }}}",
        "command10": "{{{ global.text.command10 }}}",
        "footer": "{{ global.text.footer }}",
        "url": "{{ global.text.url }}"
      }
    },
    "commandments": {
      "name": "tablet",
      "children": [
        "default"
      ],
      "value": {

      }
    }
  }
}
